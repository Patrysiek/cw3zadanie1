		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="2" GridPane.columnIndex="3"
			GridPane.rowIndex="2" type="OCT" />


		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="0" GridPane.columnIndex="3"
			GridPane.rowIndex="3" type="BIN" />
		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="1" GridPane.columnIndex="2"
			GridPane.rowIndex="2" type="BIN" />
			
