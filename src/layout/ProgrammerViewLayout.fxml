		<CustomButton maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" mnemonicParsing="false"
			text="&lt;--" GridPane.columnIndex="2" GridPane.rowIndex="3"
			type="FUNCT" />
		<CustomButton maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" mnemonicParsing="false"
			text="RESET" GridPane.rowIndex="3" type="FUNCT" />
		<CustomButton maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" mnemonicParsing="false"
			text="ZEROWANIE" GridPane.columnIndex="1" GridPane.rowIndex="3"
			type="FUNCT" />



		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="9" GridPane.columnIndex="4" type="DEC"/>
		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="8" GridPane.columnIndex="3" type="DEC"/>
		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="7" GridPane.columnIndex="2" type="OCT" />
		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="6" GridPane.columnIndex="4" type="OCT"
			GridPane.rowIndex="1" />
		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="5" GridPane.columnIndex="3" type="OCT"
			GridPane.rowIndex="1" />
		<CustomButton id="numeric"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="4" GridPane.columnIndex="2"
